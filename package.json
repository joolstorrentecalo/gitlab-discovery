{
  "name": "gitlab-mini-app",
  "label": "GitLab Mini-App",
  "version": "1.0.0",
  "description": "GitLab Discovery Mini App",
  "main": "./dist/app/static/js/nwjs.bkg.js",
  "node-remote": "*://localhost/*",
  "author": "patrickalima@nuinalp.com",
  "license": "",
  "scripts": {
    "dev": "webpack-dev-server --inline --progress --config .nwjs-config/webpack.dev.config.js",
    "start": "npm run dev:nwbkg && npm run dev",
    "build": "npm run build:vue && build --tasks win-x86 --mirror https://dl.nwjs.io/ .",
    "build:vue": "node .nwjs-config/build.js && npm run build:nwbkg",
    "dev:nwbkg": "webpack --inline --progress --env development --mode development --config .nwjs-config/webpack.bkg.config.js",
    "build:nwbkg": "node .nwjs-config/build.nw.bkg.js"
  },
  "repository": {
    "type": "git",
    "url": "https://www.gitlab.com/patrickalima98/nwjs-with-vue"
  },
  "keywords": [
    "ts",
    "typescript",
    "sass",
    "responsive",
    "nw.js",
    "vue",
    "boilerplate",
    "rot"
  ],
  "window": {
    "width": 1000,
    "height": 680
  },
  "nwVersion": "0.32.4",
  "nwPlatforms": [
    "win"
  ],
  "build": {
    "packed": false,
    "nwVersion": "0.32.4",
    "targets": [],
    "strippedProperties": [
      "build"
    ],
    "files": "./dist/app/**",
    "output": "./dist/"
  },
  "nwArchs": [
    "x64"
  ],
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
