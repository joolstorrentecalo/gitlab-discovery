    "eslint-plugin-vue": "^4.7.1",
    "event-callback-webpack-plugin": "^1.1.2",
    "figlet": "^1.2.0",
    "file-loader": "^2.0.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
