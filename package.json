    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "bugs": {
    "url": "https://gitlab.com/patrickalima98/nwjs-with-vue/issues"
  },
  "homepage": "https://gitlab.com/patrickalima98/nwjs-with-vue/tree/typescript#readme"
}
