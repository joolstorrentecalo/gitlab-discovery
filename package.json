    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "shelljs": "^0.8.2",
    "ts-loader": "^5.2.2",
    "typescript": "^3.1.3",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "url-loader": "^1.1.1",
